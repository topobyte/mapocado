<?xml version="1.0" encoding="UTF-8" ?>
<rules>
    <!-- ways -->
    <rule e="way" k="*" v="*">
        <!-- man_made features -->
        <rule e="way" k="man_made" v="pier" zoom-min="12">
            <rule closed="no" e="way" k="*" v="*">
                <class id="184"/>
                <class id="185"/>
            </rule>
            <rule closed="yes" e="way" k="*" v="*">
                <class id="186"/>
            </rule>
        </rule>
        <!-- barriers -->
        <rule e="way" k="barrier" v="*" zoom-min="14">
            <rule e="way" k="barrier" v="fence|wall|city_wall">
                <class id="187"/>
            </rule>
            <rule e="way" k="barrier" v="retaining_wall">
                <class id="188"/>
            </rule>
        </rule>
        <!-- non-physical routes -->
        <rule e="way" k="route" v="ferry">
            <class id="189"/>
        </rule>
        <!-- aerial ways -->
        <rule e="way" k="aerialway" v="*">
            <rule e="way" k="*" v="*" zoom-min="14">
                <class id="214">
                    <keep key="name"/>
                </class>
            </rule>
            <class id="209"/>
            <class id="210"/>
        </rule>
        <!-- historic -->
        <rule e="way" k="historic" v="ruins" zoom-min="17">
            <class id="246" simplify="node">
                <keep key="name"/>
            </class>
        </rule>
        <!-- place -->
        <rule e="way" k="place" v="locality" zoom-min="17">
            <class id="247" simplify="node">
                <keep key="name"/>
            </class>
        </rule>
    </rule>
</rules>
