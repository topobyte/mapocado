<?xml version="1.0" encoding="UTF-8" ?>
<rules>
    <!-- ways -->
    <rule e="way" k="*" v="*">
        <!-- landuse -->
        <rule e="way" k="landuse" v="*" zoom-min="12">
            <rule e="way" k="landuse" v="military">
                <class ref="1"/>
            </rule>
            <rule e="way" k="landuse" v="residential|farmyard">
                <class ref="2"/>
            </rule>
            <rule e="way" k="landuse" v="retail">
                <class ref="3"/>
            </rule>
            <rule e="way" k="landuse" v="industrial|brownfield|railway">
                <class ref="4"/>
            </rule>
            <rule e="way" k="landuse" v="commercial">
                <class ref="5"/>
            </rule>
            <rule e="way" k="landuse" v="construction|greenfield">
                <class ref="6"/>
            </rule>
            <rule e="way" k="landuse" v="garages">
                <class ref="7"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="8" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <!-- tunnel -->
        <rule e="way" k="tunnel" v="true|yes">
            <!-- highway tunnels -->
            <rule e="way" k="highway" v="*">
                <!-- highway tunnel casings -->
                <rule e="way" k="highway" v="steps|footway|path" zoom-min="14">
                    <class ref="9"/>
                </rule>
                <rule e="way" k="highway" v="track|cycleway|bridleway|service" zoom-min="14">
                    <class ref="10"/>
                </rule>
                <rule e="way" k="highway" v="construction">
                    <class ref="11"/>
                </rule>
                <rule e="way" k="highway" v="road">
                    <class ref="12"/>
                </rule>
                <rule e="way" k="highway" v="pedestrian|unclassified|residential|living_street|byway" zoom-min="14">
                    <class ref="13"/>
                </rule>
                <rule e="way" k="highway" v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                    <class ref="14"/>
                </rule>
                <rule e="way" k="highway" v="motorway">
                    <class ref="15"/>
                </rule>
                <!-- highway tunnel cores -->
                <rule e="way" k="highway" v="steps|footway|path" zoom-min="14">
                    <class ref="16"/>
                </rule>
                <rule e="way" k="highway" v="track" zoom-min="12">
                    <class ref="17"/>
                </rule>
                <rule e="way" k="highway" v="cycleway|bridleway|service" zoom-min="14">
                    <class ref="18"/>
                </rule>
                <rule e="way" k="highway" v="construction">
                    <class ref="19"/>
                </rule>
                <rule e="way" k="highway" v="road">
                    <class ref="20"/>
                </rule>
                <rule e="way" k="highway" v="pedestrian|unclassified|residential|living_street|byway" zoom-min="14">
                    <class ref="21"/>
                </rule>
                <rule e="way" k="highway" v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                    <class ref="22"/>
                </rule>
                <rule e="way" k="highway" v="motorway">
                    <class ref="23"/>
                </rule>
            </rule>
            <!-- railway tunnel -->
            <rule e="way" k="railway" v="*">
                <rule e="way" k="railway" v="tram|subway|light_rail|narrow_gauge">
                    <class ref="24"/>
                </rule>
                <rule e="way" k="railway" v="rail">
                    <class ref="25"/>
                </rule>
            </rule>
        </rule>
        <!-- landuse -->
        <rule e="way" k="landuse" v="*" zoom-min="12">
            <rule e="way" k="landuse" v="landfill|quarry">
                <class ref="26"/>
            </rule>
            <rule e="way" k="landuse" v="cemetery">
                <class ref="27"/>
            </rule>
            <rule e="way" k="landuse" v="field|farm|farmland|meadow|grass">
                <class ref="28"/>
            </rule>
            <rule e="way" k="landuse" v="allotments|village_green|recreation_ground">
                <class ref="29"/>
            </rule>
            <rule e="way" k="landuse" v="reservoir|basin">
                <class ref="30"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="31" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <rule e="way" k="natural|landuse" v="forest|wood|scrub" zoom-min="12">
            <rule e="way" k="wood" v="*">
                <rule e="way" k="wood" v="coniferous">
                    <class ref="32"/>
                </rule>
                <rule e="way" k="wood" v="deciduous">
                    <class ref="33"/>
                </rule>
                <rule e="way" k="wood" v="mixed">
                    <class ref="34"/>
                </rule>
            </rule>
            <rule e="way" k="wood" v="~">
                <class ref="35"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="36" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <!-- amenity -->
        <rule e="way" k="amenity" v="*" zoom-min="13">
            <rule e="way" k="amenity" v="kindergarten|school|college|university">
                <rule e="way" k="building" v="~">
                	<class ref="37"/>
            	</rule>
            </rule>
            <rule e="way" k="amenity" v="grave_yard">
                <class ref="38"/>
            </rule>
            <rule e="way" k="amenity" v="parking">
                <rule e="way" k="access" v="private" zoom-min="15">
                    <class ref="40"/>
                </rule>
                <rule e="way" k="*" v="*" zoom-min="17">
                    <class ref="41"/>
                </rule>
                <class ref="39"/>
            </rule>
            <rule closed="yes" e="way" k="amenity" v="fountain">
                <rule e="way" k="*" v="*" zoom-min="17">
                    <class ref="43" simplify="node"/>
                </rule>
                <class ref="42"/>
            </rule>
            <rule e="way" k="amenity" v="hospital" zoom-min="15">
                <class ref="44"/>
            </rule>
            <rule e="way" k="amenity" v="theatre" zoom-min="17">
                <class ref="45"/>
            </rule>
            <rule e="way" k="amenity" v="toilets" zoom-min="17">
                <class ref="46"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="17">
                <class ref="47" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <!-- natural -->
        <rule e="way" k="natural" v="*" zoom-min="12">
            <rule e="way" k="natural" v="coastline">
                <rule closed="no" e="way" k="*" v="*">
                    <class ref="48"/>
                </rule>
                <rule closed="yes" e="way" k="*" v="*">
                    <class ref="49"/>
                </rule>
            </rule>
            <rule e="way" k="natural" v="glacier">
                <class ref="50"/>
            </rule>
            <rule e="way" k="natural" v="land">
                <class ref="51"/>
            </rule>
            <rule e="way" k="natural" v="beach">
                <class ref="52"/>
            </rule>
            <rule e="way" k="natural" v="heath">
                <class ref="53"/>
            </rule>
            <rule e="way" k="natural" v="marsh|wetland">
                <class ref="54"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="55" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <!-- leisure -->
        <rule e="way" k="leisure" v="*" zoom-min="12">
            <rule e="way" k="leisure" v="park|garden|golf_course|common|green">
                <class ref="56"/>
            </rule>
            <rule e="way" k="leisure" v="playground|playing_fields|pitch|dog_park">
                <rule e="way" k="*" v="playground" zoom-min="17">
                    <class ref="58"/>
                </rule>
                <class ref="57"/>
            </rule>
            <rule e="way" k="leisure" v="nature_reserve">
                <class ref="59"/>
            </rule>
            <rule e="way" k="leisure" v="stadium|sports_centre|water_park">
                <class ref="60"/>
            </rule>
            <rule e="way" k="leisure" v="track">
                <rule e="way" k="area" v="yes|true">
                    <class ref="61"/>
                </rule>
                <rule e="way" k="area" v="~|no|false">
                    <class ref="62"/>
                    <class ref="63"/>
                </rule>
            </rule>
            <rule e="way" k="leisure" v="swimming_pool">
                <class ref="64"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="65" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <!-- waterways -->
        <rule e="way" k="waterway" v="*">
            <!-- waterway casings -->
            <rule e="way" k="waterway" v="drain">
                <class ref="66"/>
            </rule>
            <rule e="way" k="waterway" v="canal">
                <class ref="67"/>
            </rule>
            <!-- waterway cores -->
            <rule e="way" k="waterway" v="ditch">
                <class ref="68"/>
            </rule>
            <rule e="way" k="waterway" v="drain">
                <class ref="69"/>
            </rule>
            <rule e="way" k="waterway" v="canal">
                <class ref="70"/>
            </rule>
            <rule e="way" k="waterway" v="stream">
                <class ref="71"/>
            </rule>
            <rule e="way" k="waterway" v="river">
                <class ref="72"/>
            </rule>
            <rule e="way" k="waterway" v="dock">
                <class ref="73"/>
            </rule>
            <rule e="way" k="waterway" v="riverbank">
                <class ref="74"/>
            </rule>
            <rule e="way" k="waterway" v="weir">
                <class ref="75"/>
            </rule>
            <rule e="way" k="waterway" v="dam">
                <class ref="76"/>
            </rule>
            <rule e="way" k="lock" v="yes|true">
                <class ref="77"/>
                <class ref="78"/>
            </rule>
        </rule>
        <!-- military -->
        <rule e="way" k="military" v="*">
            <class ref="79"/>
        </rule>
        <!-- sport -->
        <rule e="way" k="sport" v="*" zoom-min="12">
            <rule e="way" k="sport" v="soccer" zoom-min="17">
                <class ref="80"/>
            </rule>
            <rule e="way" k="sport" v="swimming|canoe|diving|scuba_diving">
                <class ref="81"/>
            </rule>
            <rule e="way" k="sport" v="tennis">
                <rule e="way" k="*" v="*" zoom-min="17">
                    <class ref="83"/>
                </rule>
                <class ref="82"/>
            </rule>
        </rule>
        <!-- tourism areas -->
        <rule e="way" k="tourism" v="*" zoom-min="12">
            <rule e="way" k="tourism" v="attraction">
                <class ref="84"/>
            </rule>
            <rule e="way" k="tourism" v="zoo|picnic_site|caravan_site|camp_site">
                <class ref="85"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="86" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
        <rule e="way" k="natural" v="water">
            <class ref="87"/>
        </rule>
        <!-- highway area -->
        <rule e="way" k="area" v="yes|true">
            <rule e="way" k="highway" v="*">
                <rule e="way" k="highway" v="footway|footpath" zoom-min="12">
                    <class ref="88"/>
                </rule>
                <rule e="way" k="highway" v="pedestrian">
                    <class ref="89"/>
                </rule>
                <rule e="way" k="highway" v="path">
                    <class ref="90"/>
                </rule>
                <rule e="way" k="highway" v="service" zoom-min="14">
                    <class ref="91"/>
                </rule>
                <rule e="way" k="highway" v="unclassified">
                    <class ref="92"/>
                </rule>
                <rule e="way" k="highway" v="residential" zoom-min="14">
                    <class ref="93"/>
                </rule>
                <rule e="way" k="highway" v="road">
                    <class ref="94"/>
                </rule>
                <rule e="way" k="highway" v="living_street">
                    <class ref="95"/>
                </rule>
            </rule>
        </rule>
        <!-- platform cores -->
        <rule e="way" k="highway|railway|public_transport" v="platform">
            <rule closed="yes" e="way" k="*" v="*">
                <class ref="96"/>
            </rule>
            <rule closed="no" e="way" k="*" v="*">
                <class ref="97"/>
            </rule>
        </rule>
        <!-- runways casings and areas -->
        <rule e="way" k="aeroway" v="*">
            <rule closed="yes" e="way" k="aeroway" v="aerodrome">
                <class ref="98"/>
            </rule>
            <rule e="way" k="aeroway" v="apron">
                <class ref="99"/>
            </rule>
            <rule e="way" k="aeroway" v="terminal">
                <class ref="100"/>
            </rule>
            <rule e="way" k="aeroway" v="runway">
                <class ref="101"/>
            </rule>
            <rule e="way" k="aeroway" v="taxiway">
                <class ref="102"/>
            </rule>
            <rule e="way" k="*" v="*" zoom-min="16">
                <class ref="103" simplify="node">
                    <keep key="name"/>
                </class>
            </rule>
        </rule>
    </rule>
    <!-- turning circles -->
    <rule e="node" k="highway" v="turning_circle" zoom-min="14">
        <class ref="104"/>
    </rule>
    <!-- ways -->
    <rule e="way" k="*" v="*">
        <!-- no tunnel -->
        <rule e="way" k="tunnel" v="~|no|false">
            <!-- no area -->
            <rule e="way" k="area" v="~|no|false">
                <!-- highway -->
                <rule e="way" k="highway" v="*">
                    <!-- no bridge -->
                    <rule e="way" k="bridge" v="~|no|false">
                        <!-- highway casings -->
                        <rule e="way" k="highway" v="steps" zoom-min="14">
                            <class ref="105"/>
                        </rule>
                        <rule e="way" k="highway" v="footway|path" zoom-min="14">
                            <class ref="106"/>
                        </rule>
                        <rule e="way" k="highway" v="track" zoom-min="12">
                            <rule e="way" k="tracktype" v="grade1">
                                <class ref="107"/>
                            </rule>
                            <rule e="way" k="tracktype" v="grade2">
                                <class ref="108"/>
                            </rule>
                            <rule e="way" k="tracktype" v="grade3">
                                <class ref="109"/>
                            </rule>
                            <rule e="way" k="tracktype" v="grade4">
                                <class ref="110"/>
                            </rule>
                            <rule e="way" k="tracktype" v="grade5">
                                <class ref="111"/>
                            </rule>
                            <rule e="way" k="tracktype" v="~|no|false">
                                <class ref="112"/>
                            </rule>
                        </rule>
                        <rule e="way" k="highway" v="cycleway|bridleway|service" zoom-min="14">
                            <class ref="113"/>
                        </rule>
                        <rule e="way" k="highway" v="byway">
                            <class ref="114"/>
                        </rule>
                        <rule e="way" k="highway" v="pedestrian|unclassified|residential|living_street" zoom-min="14">
                            <class ref="115"/>
                        </rule>
                        <rule e="way" k="highway" v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                            <class ref="116"/>
                        </rule>
                        <rule e="way" k="highway" v="motorway">
                            <class ref="117"/>
                        </rule>
                    </rule>
                    <!-- bridge -->
                    <rule e="way" k="bridge" v="yes|true">
                        <!-- highway casings -->
                        <rule e="way" k="highway" v="steps|footway|path" zoom-min="14">
                            <class ref="118"/>
                        </rule>
                        <rule e="way" k="highway" v="track|cycleway|bridleway|service" zoom-min="14">
                            <class ref="119"/>
                        </rule>
                        <rule e="way" k="highway" v="construction">
                            <class ref="120"/>
                        </rule>
                        <rule e="way" k="highway" v="road">
                            <class ref="121"/>
                        </rule>
                        <rule e="way" k="highway" v="pedestrian|unclassified|residential|living_street|byway" zoom-min="14">
                            <class ref="122"/>
                        </rule>
                        <rule e="way" k="highway" v="tertiary|secondary_link|primary_link|trunk_link|motorway_link|secondary|primary|trunk">
                            <class ref="123"/>
                        </rule>
                        <rule e="way" k="highway" v="motorway">
                            <class ref="124"/>
                        </rule>
                    </rule>
                </rule>
            </rule>
        </rule>
        <!-- building -->
        <rule e="way" k="building" v="*" zoom-min="15">
            <rule e="way" k="*" v="*" zoom-min="17">
                <class ref="126" simplify="node">
                    <keep key="name"/>
                </class>
                <class ref="127" simplify="node">
                    <keep key="addr:housenumber"/>
                </class>
            </rule>
            <class ref="125"/>
        </rule>
        <rule e="way" k="highway" v="*">
            <!-- no tunnel -->
            <rule e="way" k="tunnel" v="~|no|false">
                <!-- no area -->
                <rule e="way" k="area" v="~|no|false">
                    <!-- highway cores -->
                    <rule e="way" k="highway" v="steps" zoom-min="14">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="129">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="128"/>
                    </rule>
                    <rule e="way" k="highway" v="footway" zoom-min="14">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="131">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="130"/>
                    </rule>
                    <rule e="way" k="highway" v="path" zoom-min="14">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="133">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="132"/>
                    </rule>
                    <rule e="way" k="highway" v="track" zoom-min="12">
                        <rule e="way" k="tracktype" v="grade1">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="135">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="134"/>
                        </rule>
                        <rule e="way" k="tracktype" v="grade2">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="137">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="136"/>
                        </rule>
                        <rule e="way" k="tracktype" v="grade3">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="139">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="138"/>
                        </rule>
                        <rule e="way" k="tracktype" v="grade4">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="141">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="140"/>
                        </rule>
                        <rule e="way" k="tracktype" v="grade5">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="143">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="142"/>
                        </rule>
                        <rule e="way" k="tracktype" v="~|no|false">
                            <rule e="way" k="*" v="*" zoom-min="15">
                                <class ref="145">
                                    <keep key="name"/>
                                </class>
                            </rule>
                            <class ref="144"/>
                        </rule>
                    </rule>
                    <rule e="way" k="highway" v="cycleway">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="147">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="146"/>
                    </rule>
                    <rule e="way" k="highway" v="bridleway">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="149">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="148"/>
                    </rule>
                    <rule e="way" k="highway" v="service" zoom-min="14">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="151">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="150"/>
                    </rule>
                    <rule e="way" k="highway" v="construction">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="153">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="152"/>
                    </rule>
                    <rule e="way" k="highway" v="road">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="155">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="154"/>
                    </rule>
                    <rule e="way" k="highway" v="pedestrian">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="157">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="156"/>
                    </rule>
                    <rule e="way" k="highway" v="unclassified|residential|living_street" zoom-min="14">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="159">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="158"/>
                    </rule>
                    <rule e="way" k="highway" v="byway">
                        <rule e="way" k="*" v="*" zoom-min="15">
                            <class ref="161">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="160"/>
                    </rule>
                    <rule e="way" k="highway" v="tertiary">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="163">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="162"/>
                    </rule>
                    <rule e="way" k="highway" v="secondary_link">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="165">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="164"/>
                    </rule>
                    <rule e="way" k="highway" v="primary_link">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="167">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="166"/>
                    </rule>
                    <rule e="way" k="highway" v="trunk_link">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="169">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="168"/>
                    </rule>
                    <rule e="way" k="highway" v="motorway_link">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="171">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="170"/>
                    </rule>
                    <rule e="way" k="highway" v="secondary">
                        <rule e="way" k="*" v="*" zoom-min="14">
                            <class ref="173">
                                <keep key="name"/>
                            </class>
                        </rule>
                        <class ref="172"/>
                    </rule>
